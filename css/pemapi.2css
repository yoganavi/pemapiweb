.seca{
		height: 100vh;
		/* height: 48rem; */
		/* max-height: 850px; */
		max-height: 50rem;
		background: #009FCA url(/img/bg/background\ app.png);
		background-size: 100% 100%;
		display: grid;
		align-items: end;
		/* grid-template-columns: .5fr 43rem 1fr; */
		grid-template-columns: .5fr 1.2fr 1fr;
		justify-items: center;
	}
	.seca p{
		color: white;
	}
	.a1{
		margin: 0 7rem 4rem 3rem;
		/* background: #000; */
	}
	.a1 img{
		width: 55%;
		display: block;
		margin: 0 auto;
	}
	.a1 p{
		text-transform: uppercase;
		font-weight: 400;
		line-height: 45px;
	}
	.a1 p:nth-of-type(1){
		font-size: 3.5rem;
		margin: 25px 0 0 0;
		font-weight: 500;
	}
	.a1 p:nth-of-type(2){
		font-size: 2rem;
	}
	.a2{
		/* background: chartreuse; */
		width: 100%;
		max-width: 48rem;
		margin: 0 0 4rem 0;
	}

	.a2 img{
		width: 100%;
	}
	.a3{
		/* background: #000; */
		align-self: end;
		/* justify-self: start; */
		margin: 0 5rem 14rem 5rem;
	}
	.a3 p{
		font-size: var(--fsML);
		text-transform: capitalize;
		/* font-weight: 400; */
	}

